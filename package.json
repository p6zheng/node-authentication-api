{
  "name": "node-authentication-api",
  "version": "1.0.0",
  "description": "API for node authentication",
  "scripts": {
    "start": "npm run build && npm run prod",
    "dev": "cross-env SERVER_PORT=3000 nodemon --watch server --exec babel-node index.js",
    "prod": "NODE_ENV=production babel-node index.js",
    "build": "webpack --config webpack.config.prod.js",
    "test": "NODE_ENV=test nodemon --exec 'mocha -R min --compilers js:babel-core/register --timeout 15000 --recursive'",
    "clean": "rimraf dist",
    "lint": "./node_modules/.bin/eslint server"
  },
  "author": "Pengfei Zheng",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "chai": "^3.5.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.2",
    "eslint": "^3.16.1",
    "eslint-config-google": "^0.7.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.1",
    "resolve-url-loader": "^2.0.2",
    "style-loader": "^0.18.1",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "bcrypt-node": "^0.1.0",
    "body-parser": "^1.15.2",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.1",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "express-session": "^1.15.2",
    "js-cookie": "^2.1.3",
    "jsonwebtoken": "^7.2.1",
    "mongoose": "^4.7.6",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "passport-strategy": "^1.0.0",
    "winston": "^2.3.1"
  }
}
